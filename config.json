{
  "locals": {
    "url": "http://localhost:8080",
    "name": "chadly.net",
    "owner": "Chad Lee",
    "description": "Software Developer. Sometimes I write stuff."
  },
  "plugins": [
	"wintersmith-less",
    "wintersmith-handlebars"
  ],
  "ignore": [
	"vendor/bootstrap/less",
	"css/blog.less",
  "css/code.less"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "handlebars": {
	"layout": "layout.html",
	"partialDir": "partials",
	"helperDir": "helpers"
  }
}
