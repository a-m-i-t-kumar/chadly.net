{
  "name": "chadly.net",
  "private": true,
  "description": "Personal blog",
  "version": "1.0.0",
  "author": "Chad Lee <npm@chadly.net>",
  "homepage": "https://www.chadly.net/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chadly/chadly.net.git"
  },
  "scripts": {
    "start": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js"
  },
  "dependencies": {
    "gatsby-image": "^2.0.13",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-plugin-typography": "^2.2.4",
    "gatsby-remark-prismjs": "^3.2.0",
    "gatsby-source-contentful": "^2.0.2",
    "gatsby-transformer-remark": "^2.1.6",
    "gatsby-transformer-sharp": "^2.0.6",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.18",
    "sharp": "^0.21.0",
    "typography": "^0.16.18",
    "typography-theme-wordpress-2016": "^0.16.18"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "gatsby": "^2.0.18",
    "gh-pages": "^2.0.1",
    "prettier": "^1.14.3",
    "yargs-parser": "^11.0.0"
  }
}
